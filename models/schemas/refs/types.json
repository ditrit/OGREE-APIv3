{
    "$id": "refs/types.json",
    "definitions": {
        "color": {
            "pattern": "^[0-9a-fA-F]{6}$"
        },
        "lengthUnit": {
            "enum": ["mm", "cm", "m", "U", "OU", "tile", "t", "f"]
        },
        "vector2": {
            "pattern": "^{\"x\"\\s*:\\s*([0-9]*[.])?[0-9]+\\s*,\\s*\"y\"\\s*:\\s*([0-9]*[.])?[0-9]+}$"
        },
        "vector3": {
            "pattern": "^{\"x\"\\s*:\\s*([0-9]*[.])?[0-9]+\\s*,\\s*\"y\"\\s*:\\s*([0-9]*[.])?[0-9]+\\s*,\\s*\"z\"\\s*:\\s*([0-9]*[.])?[0-9]+}$"
        },
        "float": {
            "pattern": "^([0-9]*[.])?[0-9]+$"
        }
    }
}